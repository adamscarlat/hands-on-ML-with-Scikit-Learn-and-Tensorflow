   "source": [
    "# Finite Difference Approximation\n",
    "def f(x, y):\n",
    "  return y * x**2 + y + 2\n",
    "\n",
    "def derivative(f, x, y, x_eps, y_eps):\n",
    "  return (f(x + x_eps, y + y_eps) - f(x, y)) / (x_eps + y_eps)\n",
    "\n",
    "grad = [\n",
    "  derivative(f, 3, 4, 0.00001, 0),\n",
    "  derivative(f, 3, 4, 0, 0.00001)\n",
    "]"
   ]
